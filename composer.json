{
    "name": "gintonicweb/gintonic-cms",
    "description": "GintonicCMS plugin for CakePHP",
    "type": "cakephp-plugin",
    "homepage": "http://gintoniccms.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Philippe Lafrance",
            "homepage": "http://phillafrance.com",
            "role": "Author"
        },
        {
            "name": "GintonicWeb Contributors",
            "homepage": "http://github.com/gintonicweb/GintonicCMS/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/gintonicweb/GintonicCMS/issues",
        "source": "https://github.com/gintonicweb/GintonicCMS"
    },
    "require": {
        "php": ">=5.4",
        "cakephp/acl": "dev-master",
        "cakephp/cakephp": "~3.0",
        "FriendsOfCake/bootstrap-ui": "~0.3",
        "FriendsOfCake/crud": "~4.0",
        "FriendsOfCake/crud-view": "dev-master",
        "FriendsOfCake/search": "dev-master",
        "gintonicweb/admin-theme": "dev-master",
        "gintonicweb/payments": "dev-master",
        "gintonicweb/twbs-theme": "dev-master",
        "gintonicweb/users": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "GintonicCMS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "GintonicCMS\\Test\\": "tests"
        }
    }
}
